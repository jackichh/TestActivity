<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.testactivity.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_new_dictionary"
        android:name="com.example.testactivity.ui.newdictionary.NewDictionaryFragment"
        android:label="@string/new_dictionary"
        tools:layout="@layout/fragment_new_dictionary" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.testactivity.ui.settings.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/nav_dictionary_detail"
        android:name="com.example.testactivity.ui.dictionarydetail.DictionaryDetailFragment"
        android:label="Dictionary detail"
        tools:layout="@layout/dictionary_detail_fragment" />
